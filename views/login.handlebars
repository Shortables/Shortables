{{user.username}}
<div class="container box">
<form id="signup" name="signup" method="post" action="/login">
	
	<label for="username">User Name</label>
	<input id="login-name" name="username" type="text" />
	<br>
	<label for="password">Password</label>
	<input id="login-pw" name="password" type="password" />
	<br>
	<input id="login-btn" class="btn" type="submit" value="Sign In" />
</form>
</div>