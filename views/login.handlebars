{{user.username}}
<div class="container box">
<form id="signup" name="signup" method="post" action="/login">
	
	<label for="username">User Name</label>
	<input id="loginName" name="username" type="text" />
	<br>
	<label for="password">Password</label>
	<input id="loginPw" name="password" type="password" />
	<br>
	<input id="loginBtn" class="btn" type="submit" value="Sign In" />
</form>
</div>